{
  "name": "mern-stack",
  "version": "0.1.0",
  "main": "./server/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/src/"
    ]
  },
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "babel-node ./server/index.js",
    "nodemon": "nodemon --inspect ./server/index.js --exec babel-node",
    "nodemon2": "nodemon ./server/index.js --exec \"node --require dotenv/config --require @babel/register\"",
    "test-server": "jest",
    "db:seed": "nodemon ./server/scripts/seed.js --exec \"node --require dotenv/config --require @babel/register\"",
    "db:db": "nodemon ./server/scripts/seed.js --exec \"node --require dotenv/config --require @babel/register\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "nodemon": "^1.19.0",
    "regenerator-runtime": "^0.13.2"
  }
}
